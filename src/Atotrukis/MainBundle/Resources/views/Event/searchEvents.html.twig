

{%  block searchForm %}
    {{ form_start(search, {'attr': {'novalidate': 'novalidate'}}) }}
    <div class="form-group">
        {{ form_widget(search.keywords, { 'attr': {'class': "form-control", 'placeholder': "Įveskite paieškos žodžius atskirtus kableliais"} }) }}
    </div>
    <button type="submit" class="btn btn-default">Search</button>
    {{ form_rest(search) }}
{% endblock %}