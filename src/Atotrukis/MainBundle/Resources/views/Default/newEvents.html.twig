<div class="news">
    {%  for event in events %}
        <a href="{{ path('_show_event', { 'eventId': event.id }) }}">
            <div class="row">
                <div class="newEventListItem">
                    <div class="col-md-6">
                        <h5>{{ event.name }}<br><small>{{ event.createdOn|date("Y-m-d h:m") }}</small></h5>
                    </div>
                    <div class="col-md-6">
                       <span class="glyphicon glyphicon-chevron-right newsIndicator"></span>
                    </div>
                </div>
            </9
        </a>
    {% endfor %}
</div>